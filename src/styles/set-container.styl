@import "./colors.styl"

.mainbody, .nav-header, .footer
  padding 0px

.margin-child
  margin-left 0px;
  margin-right 0px;

.set-container-box
  min-width 660px
  max-width 700px
  position absolute

.set-container-box__services
  background-color $defaultReverseTextColor
  padding-left 0px
  padding-right 0px
  min-width 500px
  min-height 350px
  border 1px solid $borderBoxColor
  border-radius 17px
  -moz-border-radius 17px
  -webkit-border-radius 17px

.animate-enter, .animate-leave
  transition 500ms ease-in all
  position relative
  display block